{
   "kind": "List",
   "apiVersion": "v1",
   "metadata": {
      "name": "baseimage"
   },
   "items": [
      {
         "kind": "BuildConfig",
         "apiVersion": "v1",
         "metadata": {
            "name": "baseimage",
            "namespace": "default"
         },
         "spec": {
            "triggers": [
               {
                  "type": "GitHub",
                  "github": {
                     "secret": "secret"
                  }
               },
               {
                  "type": "Generic",
                  "generic": {
                     "secret": "secret"
                  }
               },
               {
                  "type": "ImageChange",
                  "imageChange": {}
               }
            ],
            "source": {
               "type": "Git",
               "git": {
                  "uri": "https://github.com/jorgemoralespou/openshift-chained-builds.git",
                  "ref": "master"
               },
               "contextDir": "base_image"
            },
            "strategy": {
               "type": "Docker",
               "dockerStrategy": {
                  "from": {
                     "kind": "ImageStreamTag",
                     "name": "centos:centos7",
                     "namespace": "openshift"
                  }
               }
            },
            "output": {
               "to": {
                  "kind": "ImageStreamTag",
                  "name": "baseimage:latest",
                  "namespace": "openshift"
               }
            },
            "resources": {}
         }
      },
      {
         "kind": "ImageStream",
         "apiVersion": "v1",
         "metadata": {
            "name": "centos"
         },
         "spec": {
            "dockerImageRepository": "library/centos:centos7"
         }
      },
      {
         "kind": "ImageStream",
         "apiVersion": "v1",
         "metadata": {
            "name": "baseimage"
         },
         "spec": {
            "dockerImageRepository": "",
            "tags": [
               {
                  "name": "latest",
                  "annotations": {
                     "description": "Base Image",
                     "tags": "base_image",
                     "version": "1.0"
                  }
               }
            ]
         }
      }
   ]
}
